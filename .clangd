If:
  PathMatch: *.(cpp|cxx|ixx|hpp|hxx)
CompileFlags:
  Add: [-std=c++26, -I./, -W, -Wall, -Wextra, -Wpedantic, -Wno-unused-variable,-Wno-infinite-recursion,-Werror=implicit-function-declaration,-Wshadow,-Wno-shadow-field-in-constructor-modified,-Wno-shadow-ivar,-Wuninitialized,-Wunused-label,-Wunused-lambda-capture]
  CompilationDatabase: ./cmake-build-debug/compile_commands.json
Index:
  Background: Build
  StandardLibrary: Yes
Hover:
  ShowAKA: Yes
